{
  "name": "ncom",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "yarn run generate:pwa-icon && vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "cordova-build-android": "cross-env CORDOVA_PLATFORM=android vue-cli-service cordova-build-android",
    "cordova-build-ios": "cross-env CORDOVA_PLATFORM=ios vue-cli-service cordova-build-ios",
    "cordova-build-only-www-android": "cross-env CORDOVA_PLATFORM=android vue-cli-service cordova-build-only-www-android",
    "cordova-build-only-www-ios": "cross-env CORDOVA_PLATFORM=ios vue-cli-service cordova-build-only-www-ios",
    "cordova-prepare": "vue-cli-service cordova-prepare",
    "cordova-serve-android": "cross-env CORDOVA_PLATFORM=android vue-cli-service cordova-serve-android",
    "cordova-serve-ios": "cross-env CORDOVA_PLATFORM=ios vue-cli-service cordova-serve-ios",
    "electron:build": "yarn run generate-ico && vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "i18n:report": "vue-cli-service i18n:report --src './src/**/*.?(js|vue)' --locales './src/locales/**/*.json'",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "generate:icon": "yarn run generate:pwa-icon && yarn generate:cordova-icon",
    "generate:pwa-icon": "npx pwa-asset-generator src/assets/icon.png public/img --opaque false --icon-only --favicon --type png --padding 0",
    "generate:cordova-icon": "yarn run resize-icon && cd src-cordova && yarn generate:icon",
    "resize-icon": "npx resize ./src/assets/icon.png -w 2000 -h 2000 -o ./src-cordova/",
    "generate-ico": "png-to-ico ./src/assets/icon.png > ./src/assets/icon.ico"
  },
  "main": "background.js",
  "dependencies": {
    "@mdi/font": "^3.6.95",
    "animate.css": "^3.7.2",
    "artefact-controller": "0.0.5",
    "axios": "^0.19.2",
    "balloon-css": "^1.0.4",
    "core-js": "^3.9.1",
    "ionicons": "4.6.3",
    "jquery": "^3.6.0",
    "material-design-icons-iconfont": "^5.0.1",
    "moonbase-labs": "^0.2.1",
    "phaser": "^3.53.1",
    "pwa-asset-generator": "^3.2.2",
    "register-service-worker": "^1.7.2",
    "resize-cli": "^0.0.4",
    "roboto-fontface": "*",
    "vue": "^2.6.11",
    "vue-codemirror": "^4.0.6",
    "vue-i18n": "^8.24.1",
    "vue-mq": "^1.0.1",
    "vue-router": "^3.5.1",
    "vuedraggable": "^2.23.2",
    "vuetify": "^2.4.6",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@intlify/vue-i18n-loader": "^1.1.0",
    "@vue/cli-plugin-babel": "^4.5.11",
    "@vue/cli-plugin-pwa": "~4.5.0",
    "@vue/cli-plugin-router": "^4.5.11",
    "@vue/cli-plugin-unit-jest": "^4.5.11",
    "@vue/cli-plugin-vuex": "^4.5.11",
    "@vue/cli-service": "^4.5.11",
    "@vue/test-utils": "^1.1.3",
    "electron": "^12.0.1",
    "electron-devtools-installer": "^3.1.0",
    "node-sass": "^4.12.0",
    "png-to-ico": "^2.1.2",
    "sass": "^1.32.8",
    "sass-loader": "^8.0.2",
    "spectron": "^11.0.0",
    "vue-cli-plugin-cordova": "~2.4.1",
    "vue-cli-plugin-electron-builder": "^2.0.0-rc.6",
    "vue-cli-plugin-i18n": "~1.0.1",
    "vue-cli-plugin-vuetify": "~2.0.7",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.7.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "jest": {
    "preset": "@vue/cli-plugin-unit-jest"
  },
  "description": "",
  "author": ""
}
